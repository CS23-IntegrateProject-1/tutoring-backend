generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model restaurants {
  restaurantId        Int     @id @default(autoincrement())
  restaurant_name     String? @db.VarChar(255)
  restaurant_location String? @db.VarChar(255)
  numberOfEmployee    Int?
}
